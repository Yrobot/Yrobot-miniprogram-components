.YrobotPageHolder{
    position: absolute;
    width: 100%;
    height: 100%;
    left: 0;
    top: 0;
}
.YrobotContentHolder{
    position: absolute;
    width: 100%;
    height: 100%;
    left: 0;
    top: 0;
    overflow-x: hidden;
    overflow-y: scroll;
    z-index: 1;
}
.YrobotPopHolder{
    position: absolute;
    width: 100%;
    height: 100%;
    left: 0;
    top: 0;
    z-index: auto;
}