.YrobotPageHolder{
    /* fixed解决iPhone页面上下滑动的弹簧效果 */
    position: fixed; 
    width: 100%;
    left: 0;
    top: 0;
    bottom: 0;
    overflow: hidden;
}
.YrobotContentHolder{
    position: absolute;
    width: 100%;
    height: 100%;
    left: 0;
    top: 0;
    overflow-x: hidden;
    overflow-y: scroll;
    z-index: 1;
}
.YrobotPopHolder{
    position: absolute;
    width: 100%;
    height: 100%;
    left: 0;
    top: 0;
    z-index: auto;
}