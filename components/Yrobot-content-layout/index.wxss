.contentLayout{
    position: absolute;
    left: 0;
    top: 0;
    width: 100%;
    height: 100%;
}
.windowHolder{
    position: absolute;
    left: 0;
    top: 0;
    width: 100%;
    height: 100%;
    display: flex;
    flex-direction: column;
}
.statusPlaceHolder{
    width: 100%;
    flex: none;
}
.navigationPlaceHolder{
    position: relative;
    width: 100%;
    flex: none;
}
.contentWindow{
    position: relative;
    width: 100%;
    overflow-x: hidden;
    overflow-y: scroll;
    flex: auto;
}
.tabBarPlaceHolder{
    position: relative;
    width: 100%;
    flex: none;
}

.statusBar{
    position: absolute;
    width: 100%;
    left: 0;
    top: 0;
    /* background-color: rgb(82, 81, 81); */
}
.navigationBarHolder{
    position: absolute;
    left: 0;
    width: 100%;
}
.tabBarHolder{
    position: absolute;
    left: 0;
    bottom: 0;
    width: 100%;
}



.inner{
    width: 100%;
    height: 80rpx;
    background-color: rgb(209, 112, 247);
}