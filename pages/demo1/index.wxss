.contentWindow {
  width: 100%;
  height: 100%;
  position: absolute;
  left: 0;
  top: 0;
  overflow-x: hidden;
  overflow-y: scroll;
}
.popHolder {
  position: absolute;
  width: 100%;
  height: 100%;
  left: 0;
  top: 0;
  z-index: auto;
}
.left {
  width: 750rpx;
  height: 110rpx;
  line-height: 110rpx;
  box-sizing: border-box;
  padding: 0 20rpx;
}
.right {
  width: 300rpx;
  height: 110rpx;
  line-height: 110rpx;
  display: flex;
  text-align: center;
}
.right .edit {
  flex: 5;
  height: 100%;
  background-color: #60c526;
  color: #fff;
}
.right .delete {
  flex: 4;
  height: 100%;
  background-color: #df1f1a;
  color: #fff;
}
.myMap {
  position: absolute;
  width: 100%;
  height: 100%;
}
.holder {
  position: absolute;
  width: 100%;
  height: 100%;
  background-color: rgba(247, 132, 132, 0.5);
}
.cover {
  position: absolute;
  left: 25%;
  top: 40%;
  width: 50%;
  height: 300rpx;
  background-color: #b4e45c;
}
.inner {
  width: 100%;
  height: 100rpx;
  background-color: #4d67f7;
}
