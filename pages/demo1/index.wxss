.listView {
  width: 100%;
  height: 100%;
  position: relative;
  overflow-x: hidden;
  overflow-y: scroll;
}
.left {
  width: 750rpx;
  height: 110rpx;
  line-height: 110rpx;
  background-color: #0396e4;
  box-sizing: border-box;
  padding: 0 20rpx;
}
.right {
  width: 300rpx;
  height: 110rpx;
  line-height: 110rpx;
  display: flex;
  text-align: center;
}
.right .edit {
  flex: 5;
  height: 100%;
  background-color: #60c526;
  color: #fff;
}
.right .delete {
  flex: 4;
  height: 100%;
  background-color: #df1f1a;
  color: #fff;
}
